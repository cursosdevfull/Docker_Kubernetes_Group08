{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "es2020",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": "./",
    "outDir": "./dist"
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src/**/*.ts"]
}
